            <string>if(free == true){
instance = instance_nearest(x, y, obj_prototype_enemy);
if(keyboard_check(vk_down) || keyboard_check(ord('S')))
{
    phy_rotation = 90;
    phy_position_y += speed;
}

if(keyboard_check(vk_up) || keyboard_check(ord('W')))
{
    phy_rotation = 270;
    phy_position_y -= speed;
}

if(keyboard_check(vk_right) || keyboard_check(ord('D')))
{
    phy_rotation = 0;
    phy_position_x += speed;
}

if(keyboard_check(vk_left) || keyboard_check(ord('A')))
{
    phy_rotation = 180;
    phy_position_x -= speed;
}



//if(collision_circle(phy_position_x, phy_position_y, sprite_width*0.5, obj_prototype_hiding_obstacle, false, false)) 
var inst;
inst = collision_circle(phy_position_x, phy_position_y, sprite_width*0.5, obj_prototype_hiding_obstacle, true, false);
if(inst != noone) {
    if(inst.bbox_left - margin &lt;= phy_position_x-sprite_width*0.5 &amp;&amp; inst.bbox_top - margin &lt;= phy_position_y-sprite_height*0.5 &amp;&amp; inst.bbox_right + margin &gt;= phy_position_x+sprite_width*0.5 &amp;&amp; inst.bbox_bottom + margin &gt;= phy_position_y+sprite_height*0.5)
    {
        isHiding = true;
        isStealthing = true;
        isRunning = false;
    } 
} else {
    if(keyboard_check(ord("H"))) {
        speed = 1;
        isStealthing = true;
        isRunning = false;
    } else if(keyboard_check(vk_shift)){
        isRunning = true;
        isStealthing = false;
        speed = 3 ;
    } else {
        speed = 2;
        isStealthing = false;
        isRunning = false;
    }
    isHiding = false;
}
if(instance_place(phy_position_x, phy_position_y, obj_prototype_celldoor)) {
    isTouching = true;
} else {
    isTouching = false;
}
}
</string>
